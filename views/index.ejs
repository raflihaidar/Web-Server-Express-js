<div class="container text-center">
  <h1>WEB SERVER BY EXPRESS JS</h1>
  <h2>Daftar Mahasiswa Telkom University</h2>
  <div class="text-start">
    <% data.forEach(item => { %>
    <li>Nama : <%= item.Nama %></li>
    <li>NIM : <%= item.NIM %></li>
    <li>Jurusan : <%= item.Jurusan %></li>
    <button
      class="btn bg-danger text-light"
      onclick="location.href='/<%=item.NIM %>'"
    >
      <a>detail</a>
    </button>
    <br />
    <br />
    <% }) %>
  </div>
</div>
